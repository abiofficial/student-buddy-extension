{
  "manifest_version": 3,
  "name": "Student Buddy",
  "version": "1.1",
  "description": "A personalized student productivity extension with To-Do, Notes, Timer, and Resources.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  }
}